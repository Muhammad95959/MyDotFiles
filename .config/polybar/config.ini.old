include-file = ./colors/catpuccin_mocha.ini

[colors]
foreground = #ffffff
disabled = #777777
alert = #e60053

[bar/main]
monitor = eDP-1
modules-left = launcher i3 add_i3_workspace space polywins
modules-right = xkeyboard space cpu space memory space backlight space pulseaudio space battery space date space wlan space powermenu
font-0 = "Symbols Nerd Font:style=2048-em:size=12;2"
font-1 = "Fantasque Sans Mono:style=Bold:pixelsize=10;2"
font-2 = "RobotoMono Nerd Font:style=Bold:size=11;2"
font-3 = "Noto Color Emoji:pixelsize=16:style=Regular:scale=10;2"
font-4 = "Symbols Nerd Font:style=2048-em:size=13;2"
font-5 = "Symbols Nerd Font:style=2048-em:size=10;2"
; background = #881e1e2e
background = ${colors.base}
foreground = ${colors.foreground}
cursor-click = pointer
cursor-scroll = ns-resize
enable-ipc = true
tray-position = center
line-size = 2pt
bottom = false
height = 20
; width = 80%
; radius = 10
; offset-x = 10%
; offset-y = 5
; wm-restack = i3
; override-redirect = true

[bar/secondary]
monitor = HDMI-1-0
modules-left = launcher i3 add_i3_workspace space polywins
modules-right = xkeyboard space cpu space memory space backlight space pulseaudio space battery space date space wlan space powermenu
font-0 = "Symbols Nerd Font:style=2048-em:size=12;2"
font-1 = "Fantasque Sans Mono:style=Bold:pixelsize=10;2"
font-2 = "RobotoMono Nerd Font:style=Bold:size=11;2"
font-3 = "Noto Color Emoji:pixelsize=16:style=Regular:scale=10;2"
font-4 = "Symbols Nerd Font:style=2048-em:size=13;2"
font-5 = "Symbols Nerd Font:style=2048-em:size=10;2"
; background = #881e1e2e
background = ${colors.base}
foreground = ${colors.foreground}
cursor-click = pointer
cursor-scroll = ns-resize
enable-ipc = true
tray-position = none
line-size = 2pt
bottom = false
height = 20
; width = 80%
; radius = 10
; offset-x = 10%
; offset-y = 5
; wm-restack = i3
; override-redirect = true

[module/space]
type = custom/text
content = "  "
content-foreground = ${colors.foreground}

[module/sep]
type = custom/text
content = ||
content-foreground = ${colors.foreground}

[module/launcher]
type = custom/text
content = "  ïŒƒ  "
content-foreground = ${colors.flamingo}
content-font = 5
click-left = rofi -disable-history -show drun -theme /home/muhammad/.config/rofi/side_launcher.rasi
click-right = bash ~/Scripts/open_excecutables.sh

[module/i3]
type = internal/i3
index-sort = true
pin-workspaces = true
enable-scroll = false
wrapping-scroll = false
reverse-scroll = false
fuzzy-match = true
label-mode = %mode%
label-mode-padding = 2
label-mode-margin-right = 1
label-mode-background = ${colors.alert}
label-focused = %icon%
label-focused-padding = 2
label-focused-foreground = ${colors.lavender}
label-focused-font = 5
label-unfocused = %icon%
label-unfocused-padding = 2
label-unfocused-font = 5
label-urgent = %icon%
label-urgent-padding = 4
label-urgent-foreground = #000000
label-urgent-background = ${colors.alert}
ws-icon-0 = 1;ó°² 
ws-icon-1 = 2;ó°²¢
ws-icon-2 = 3;ó°²¤
ws-icon-3 = 4;ó°²¦
ws-icon-4 = 5;ó°²¨
ws-icon-5 = 6;ó°²ª
ws-icon-6 = 7;ó°²¬
ws-icon-7 = 8;ó°²®
ws-icon-8 = 9;ó°²°
ws-icon-9 = 10;ó°¿¬
ws-icon-default = ï˜®

[module/add_i3_workspace]
type = custom/text
content = "  ó°—  "
content-foreground = ${colors.maroon}
content-font = 5
click-left = i3-msg workspace $(bash ~/Scripts/first_unused_workspace.sh)
click-right = bash ~/Scripts/open_workspace.sh
click-middle = bash ~/Scripts/move_window_to_workspace.sh

[module/polywins]
type = custom/script
exec = bash ~/.config/polybar/scripts/polywins.sh 2>/dev/null
format = <label>
label = %output%
format-font = 3
tail = true

