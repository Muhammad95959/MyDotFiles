label-foreground = ${colors.mauve}

[module/powermenu]
type = custom/text
content = "ï€‘  "
content-foreground = ${colors.pink}
click-left = bash ~/.config/rofi/scripts/powermenu/type-4/powermenu.sh

[settings]
screenchange-reload = true
