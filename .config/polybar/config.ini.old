ramp-capacity-4 = ï‰‚
ramp-capacity-5 = ï‰
ramp-capacity-6 = ï‰
ramp-capacity-7 = ï‰€
ramp-capacity-8 = ï‰€
ramp-capacity-0-foreground = ${colors.alert}
ramp-capacity-foreground = ${colors.green}
animation-charging-0 = ï‰„
